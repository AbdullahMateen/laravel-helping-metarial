{
    "name": "abdullah-mateen/laravel-helping-metarial",
    "description": "Laravel helper functions and general helping material",
    "keywords": ["laravel", "helpers"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdullah Mateen",
            "email": "abdullahmateen101@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-simplexml": "*",
        "intervention/image": "^3.3"
    },
    "require-dev": {
        "laravel/framework": "^8.37|^9.0|^10.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "AbdullahMateen\\LaravelHelpingMaterial\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "AbdullahMateen\\LaravelHelpingMaterial\\LaravelHelpingMaterialServiceProvider"
            ],
            "aliases": {
                "MediaService": "AbdullahMateen\\LaravelHelpingMaterial\\Facades\\MediaServiceFacade"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
